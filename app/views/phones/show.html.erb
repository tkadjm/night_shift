<div class="page-header">
  <h3>Phone #<%= @phone.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Site</dt>
      <dd><%= @phone.site_id %></dd>

      <dt>Number</dt>
      <dd><%= @phone.number %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/phones" class="btn btn-primary">
        Back
      </a>
      <a href="/phones/<%= @phone.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_phone/<%= @phone.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
