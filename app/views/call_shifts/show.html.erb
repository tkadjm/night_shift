<div class="page-header">
  <h3>Call Shift #<%= @call_shift.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Site</dt>
      <dd><%= @call_shift.site_id %></dd>

      <dt>Professional</dt>
      <dd><%= @call_shift.professional_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/call_shifts" class="btn btn-primary">
        Back
      </a>
      <a href="/call_shifts/<%= @call_shift.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_call_shift/<%= @call_shift.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
