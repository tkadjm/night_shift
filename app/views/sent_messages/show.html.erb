<div class="page-header">
  <h3>Sent Message #<%= @sent_message.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Message</dt>
      <dd><%= @sent_message.message_id %></dd>

      <dt>Professional</dt>
      <dd><%= @sent_message.professional_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/sent_messages" class="btn btn-primary">
        Back
      </a>
      <a href="/sent_messages/<%= @sent_message.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_sent_message/<%= @sent_message.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
